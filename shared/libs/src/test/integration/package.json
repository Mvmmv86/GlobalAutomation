{
  "name": "@tradingview-gateway/integration-tests",
  "version": "1.0.0",
  "description": "Integration tests for TradingView Gateway trading system",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:webhook": "jest --testNamePattern='Webhook'",
    "test:performance": "jest --testNamePattern='Performance'",
    "test:error": "jest --testNamePattern='Error'",
    "test:flow": "jest --testNamePattern='Flow'",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "@types/node": "^20.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
}